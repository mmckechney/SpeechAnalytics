{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "{{AZURE_LOCATION}}"
    },
    "resourceGroupName": {
      "value": "{{AZURE_RESOURCE_GROUP}}"
    },
    "storageAccountName": {
      "value": "{{APP_BASE_NAME}}storage"
    },
    "aiServicesAccountName": {
      "value": "{{APP_BASE_NAME}}-aiservices"
    },
    "functionAppName": {
      "value": "{{APP_BASE_NAME}}"
    },
    "cosmosAccountName": {
      "value": "{{APP_BASE_NAME}}-cosmos"
    },
    "aiSearchName": {
      "value": "{{APP_BASE_NAME}}-search"
    },
    "userIdGuid": {
      "value": "{{AZURE_PRINCIPAL_ID}}"
    },
    "askContainerImage": {
      "value": "{{AZD_SERVICE_ASKINSIGHTS_IMAGE}}"
    },
    "transcriptionContainerImage": {
      "value": "{{AZD_SERVICE_TRANSCRIPTION_IMAGE}}"
    },
    "containerRegistryServer": {
      "value": "{{AZURE_CONTAINER_REGISTRY_ENDPOINT}}"
    },
    "containerRegistryUsername": {
      "value": "{{AZURE_CONTAINER_REGISTRY_USERNAME}}"
    },
    "containerRegistryPassword": {
      "value": "{{AZURE_CONTAINER_REGISTRY_PASSWORD}}"
    }
  }
}